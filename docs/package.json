{
  "name": "tagged-classnames-docs",
  "private": true,
  "description": "Documentation for the tagged-classnames library",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "pnpm run prepare-content && gatsby build",
    "develop": "pnpm run prepare-content && gatsby develop",
    "start": "pnpm run prepare-content && gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "prepare-content": "pnpm run reset-external && pnpm run copy-external && pnpm run rename-license",
    "copy-external": "copyfiles -f ../src/index.ts ../README.md ../LICENSE ../CODE_OF_CONDUCT.md ../CHANGELOG.md ./external",
    "reset-external": "rimraf ./external && mkdir external",
    "rename-license": "renamer --find LICENSE --replace LICENSE.md external/LICENSE"
  },
  "dependencies": {
    "@fontsource/open-sans": "^4.5.14",
    "@mdx-js/react": "^2.3.0",
    "autoprefixer": "^10.4.13",
    "gatsby": "^5.6.1",
    "gatsby-plugin-mdx": "^5.6.0",
    "gatsby-source-filesystem": "^5.6.0",
    "prismjs": "^1.29.0",
    "prism-themes": "^1.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "tailwindcss": "^3.2.7"
  },
  "devDependencies": {
    "@types/prismjs": "^1.26.0",
    "babel-plugin-prismjs": "^2.1.0",
    "babel-preset-gatsby": "^2.14.0",
    "copyfiles": "^2.4.1",
    "gatsby-plugin-pnpm": "^1.2.10",
    "gatsby-plugin-postcss": "^6.7.0",
    "postcss": "^8.4.21",
    "renamer": "^4.0.0",
    "rimraf": "^4.1.2"
  }
}
